<html>
<head id="_head">
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <headstylesheets/>
    <link rel="stylesheet/less" href="/core/styles.less" />
    <headscripts/>
    <link href='http://fonts.googleapis.com/css?family=Ubuntu:400,700' rel='stylesheet' type='text/css' />
    <link rel="shortcut icon" href="/dl/core/ui/favicon.png" />
</head>
<body id="main" style="background:#eee">

<div id="page">
    <div id="topbar">
        <div class="container">
            <img class="logo" src="/dl/core/ui/logo.png" />
            Ajenti
            <img class="ajax-loader" src="/dl/core/ui/ajax.gif" id="ajax-loader" style="display:none"/>
        </div>
    </div>

    <div id="topbar2">
        <div class="container">
            <span id="topplaceholder-top"/>
            <span id="topplaceholder-bottom"/>
            <a class="ui-el-top-category" style="float:right" href="/logout">
                <img src="/dl/core/ui/stock/dialog-cancel.png" class="ui-el-category-icon" />
            </a>
            <a class="ui-el-top-category ui-el-top-category-nohover" style="float:right" href="#" id="cat-username">
                <img src="/dl/core/ui/stock/user.png" class="ui-el-category-icon" />
            </a>
        </div>
    </div>

    <div class="main-container">
        <div class="sidebar" id="leftplaceholder" />
   	    <div class="content" id="rightplaceholder">
	        <script>
	            ajax('/handle///nothing');
	        </script>
	    </div>
    </div>

    <div class="footer">
        <div class="container">
            <div id="version" />
            <div id="links" />
        </div>
    </div>

    <script>
        ajax('/handle///nothing');
    </script>

<div id="warningbox-all" style="display:none">
    <div class="ui-el-modal-blackout" />
    <div class="ui-el-modal-wrapper" id="warningbox-wr">
        <div class="ui-el-dialog">
            <div class="ui-el-dialog-content">
                <table>
                    <tr>
                        <td width="40">
                            <img src="/dl/core/ui/stock/warning.png" />
                        </td>
                        <td>
                            <div class="ui-el-error-title">
                                <span class="ui-el-label-3">You are about to:</span>
                            </div>
                            <div class="ui-el-error-content" style="min-width:0px">
                                <span id="warning-text" />
                            </div>
                        </td>
                    </tr>
                </table>
            </div>
            <div class="ui-el-auth-dialog-bar" />
            <div class="ui-el-modal-buttons">
                <button text="OK" id="warning-button"/>
                <a class="ui-el-button" href="#" id="{@id}" onclick="ui_hidewarning()">
                    Cancel
                </a>
            </div>
        </div>
    </div>
</div>

<div id="help-hint" class="ui-el-helphint"></div>

</div>

</body>
</html>
